<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.energize">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Energize">
        <activity android:name=".Theme_Geothermal"></activity>
        <activity android:name=".Theme_Wind" />
        <activity android:name=".Theme_Hydro" />
        <activity android:name=".Theme_Solar" />
        <activity android:name=".SolveQuiz" />
        <activity android:name=".SelectAvatar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>